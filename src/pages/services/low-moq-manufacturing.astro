  <!-- Process Section -->
  <section class="py-16 md:py-20">
    <div class="max-w-6xl mx-auto px-4 sm:px-6 lg:px-8">
      <ScrollAnimation>
        <div class="text-center mb-12">
          <h2 class="text-3xl md:text-4xl font-bold text-dark-900 mb-4">
            How Our Low MOQ Program Works
          </h2>
          <p class="text-lg text-dark-600 max-w-3xl mx-auto">
            Simple, transparent process from quote to delivery.
          </p>
        </div>
      </ScrollAnimation>

      <div class="grid md:grid-cols-4 gap-6">
        <ScrollAnimation>
          <div class="text-center">
            <div class="bg-mint-600 text-white w-16 h-16 rounded-full flex items-center justify-center text-2xl font-bold mx-auto mb-4">1</div>
            <h3 class="text-lg font-bold text-dark-900 mb-2">Submit Request</h3>
            <p class="text-dark-600">Tell us your product needs and desired quantity</p>
          </div>
        </ScrollAnimation>
        <ScrollAnimation delay={100}>
          <div class="text-center">
            <div class="bg-mint-600 text-white w-16 h-16 rounded-full flex items-center justify-center text-2xl font-bold mx-auto mb-4">2</div>
            <h3 class="text-lg font-bold text-dark-900 mb-2">Receive Quote</h3>
            <p class="text-dark-600">Get transparent pricing within 24-48 hours</p>
          </div>
        </ScrollAnimation>
        <ScrollAnimation delay={200}>
          <div class="text-center">
            <div class="bg-mint-600 text-white w-16 h-16 rounded-full flex items-center justify-center text-2xl font-bold mx-auto mb-4">3</div>
            <h3 class="text-lg font-bold text-dark-900 mb-2">Production Begins</h3>
            <p class="text-dark-600">Same GMP facilities, same quality standards</p>
          </div>
        </ScrollAnimation>
        <ScrollAnimation delay={300}>
          <div class="text-center">
            <div class="bg-mint-600 text-white w-16 h-16 rounded-full flex items-center justify-center text-2xl font-bold mx-auto mb-4">4</div>
            <h3 class="text-lg font-bold text-dark-900 mb-2">Receive Products</h3>
            <p class="text-dark-600">4-6 weeks to market-ready products</p>
          </div>
        </ScrollAnimation>
      </div>
    </div>
  </section>

  <!-- CTA Section -->
  <section class="py-16 md:py-20 bg-gradient-to-b from-mint-50 to-white">
    <div class="max-w-4xl mx-auto px-4 sm:px-6 lg:px-8 text-center">
      <ScrollAnimation>
        <h2 class="text-3xl md:text-4xl font-bold text-dark-900 mb-4">
          Ready to Start Small and Grow Big?
        </h2>
        <p class="text-xl text-dark-600 mb-8">
          Join 20+ successful brands that started with just 1000 units.
          Get your custom low MOQ quote today.
        </p>
        <div class="flex flex-col sm:flex-row gap-4 justify-center">
          <a href="/contact" class="inline-flex items-center justify-center px-8 py-4 bg-mint-600 text-white font-semibold rounded-lg hover:bg-mint-700 transition-all duration-300 transform hover:scale-105 shadow-lg hover:shadow-xl">
            Get Your Low MOQ Quote
            <svg class="ml-2 w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 8l4 4m0 0l-4 4m4-4H3"></path>
            </svg>
          </a>
          <button
            data-cal-link="Nutricraftlabs"
            data-cal-config='{"theme":"light"}'
            class="inline-flex items-center justify-center px-8 py-4 bg-white text-mint-600 font-semibold rounded-lg border-2 border-mint-600 hover:bg-mint-50 transition-colors"
          >
            Schedule Consultation
          </button>
        </div>
