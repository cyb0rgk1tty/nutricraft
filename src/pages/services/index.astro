---
import BaseLayout from "../../layouts/BaseLayout.astro";
import Header from "../../components/Header.astro";
import Footer from "../../components/Footer.astro";
import ScrollAnimation from "../../components/ScrollAnimation.astro";
import FAQ from "../../components/FAQ.astro";

// Manufacturing Services
const manufacturingServices = [
  {
    title: "Low MOQ Manufacturing",
    description: "Start with just 1000 units - 3x lower than industry standard. Perfect for startups and testing new products.",
    href: "/services/low-moq-manufacturing",
    highlights: ["1000 unit minimum", "All formats available", "Same GMP quality"],
    timeline: "8-10 weeks",
    cta: "Learn About Low MOQs",
    isComingSoon: false
  },
  {
    title: "Private & White Label Manufacturing",
    description: "Launch your supplement brand in US or Canada with custom private label or ready-made white label formulas.",
    href: "/services/private-white-label-manufacturing",
    highlights: ["Custom or stock formulas", "US & Canada compliant", "1000 unit minimum"],
    timeline: "4-16 weeks",
    cta: "Start Your Brand",
    isComingSoon: false
  },
  {
    title: "Custom Formulation",
    description: "Create unique formulas with expert R&D teams. From concept to market-ready product.",
    href: "/services/custom-formulation",
    highlights: ["Expert R&D team", "Stability testing", "Ingredient sourcing"],
    timeline: "12-16 weeks",
    cta: "Start Formulating",
    isComingSoon: false
  },
  {
    title: "GMP Partner Matching",
    description: "Connect with pre-vetted GMP-certified manufacturers that match your specific needs.",
    href: "/services/gmp-partner-matching",
    highlights: ["FDA registered", "cGMP certified", "Transparent pricing"],
    timeline: "1-2 weeks",
    cta: "Find Your Partner",
    isComingSoon: false
  },
  {
    title: "Quality Testing",
    description: "Comprehensive third-party testing ensures product safety and quality at every stage.",
    href: "/services/quality-testing",
    highlights: ["COA management", "Microbial testing", "Heavy metals"],
    timeline: "2-3 weeks",
    cta: "Ensure Quality",
    isComingSoon: false
  }
];

// Go-to-Market Services
const goToMarketServices = [
  {
    title: "NPN Applications",
    description: "Navigate Health Canada regulations with expert NPN application support for Canadian market entry.",
    href: "/services/npn-applications",
    highlights: ["Document preparation", "Health Canada liaison", "Compliance guidance"],
    timeline: "3-12 months",
    badge: "Essential for Canada",
    cta: "Get NPN Support",
    isComingSoon: false
  },
  {
    title: "Compliance Labels",
    description: "Ensure market compliance with bilingual labeling that meets all regulatory requirements.",
    href: "/services/compliance-labels",
    highlights: ["FDA compliant", "Bilingual labels", "Nutrition panels"],
    timeline: "1-2 weeks",
    cta: "Create Labels",
    isComingSoon: false
  },
  {
    title: "Fulfillment & Distribution",
    description: "Streamline logistics with our trusted 3PL partner network for warehousing and shipping.",
    href: "/services/fulfillment-distribution",
    highlights: ["3PL solutions", "Shopify store setup", "D2C fulfillment", "Amazon FBA prep"],
    timeline: "Setup in 1 week",
    cta: "Setup Fulfillment",
    isComingSoon: false
  },
  {
    title: "Marketing & Brand Kit",
    description: "Complete branding, design, and digital marketing services to launch and grow your supplement brand.",
    href: "/services/marketing-brand-kit",
    highlights: ["Product photography", "Label design", "Google & Meta ads", "Social media marketing"],
    timeline: "2-3 weeks",
    cta: "Build Your Brand",
    isComingSoon: false
  },
  {
    title: "Website Design & Development",
    description: "Launch your supplement brand online with professional website design and development tailored for e-commerce.",
    href: "/services/website-design-development",
    highlights: ["Shopify stores", "Custom websites", "Mobile-first design", "SEO optimized"],
    timeline: "3-6 weeks",
    cta: "Build Your Site",
    isComingSoon: false
  }
];

const processSteps = [
  {
    number: "01",
    title: "Discovery",
    description: "Understand your vision, target market, and requirements",
    category: "Planning",
    timeline: "1-2 weeks"
  },
  {
    number: "02",
    title: "Formulation",
    description: "Develop or select the perfect formula for your brand",
    category: "Manufacturing",
    timeline: "2-4 weeks"
  },
  {
    number: "03",
    title: "Compliance",
    description: "Ensure all regulatory requirements are met",
    category: "Manufacturing",
    timeline: "2-3 weeks"
  },
  {
    number: "04",
    title: "Production",
    description: "Manufacture with rigorous quality control",
    category: "Manufacturing",
    timeline: "4-6 weeks"
  },
  {
    number: "05",
    title: "Launch",
    description: "Bring your products to market successfully",
    category: "Go-to-Market",
    timeline: "1-2 weeks"
  },
  {
    number: "06",
    title: "Distribution",
    description: "Scale with fulfillment and logistics support",
    category: "Go-to-Market",
    timeline: "1 week"
  }
];

const faqItems = [
  {
    question: "What makes Nutricraft different from going directly to manufacturers?",
    answer: "We've pre-vetted all manufacturing partners for GMP compliance, quality standards, and reliability. Our expertise helps you avoid costly mistakes, navigate regulations efficiently, and launch 3x faster. Plus, our network gives you access to better pricing through aggregated volume. Most importantly, we save you countless hours and eliminate stress by handling all the complex paperwork, regulatory compliance, label reviews, and documentation requirements - letting you focus on building your brand instead of drowning in administrative tasks."
  },
  {
    question: "Do I need both Manufacturing and Go-to-Market services?",
    answer: "Not necessarily. While our end-to-end solution provides the smoothest path to market, you can choose only the services you need. Many clients start with manufacturing and add go-to-market services as they scale."
  },
  {
    question: "What are your minimum order quantities (MOQs)?",
    answer: "We offer the industry's lowest MOQs starting at just 1000 units - that's 3-5x lower than most manufacturers who require 5000+ units. This allows you to test products, manage cash flow better, and scale gradually."
  },
  {
    question: "How long does it take to launch a supplement brand?",
    answer: "With white label formulas (US market): 8-10 weeks from order to delivery. For Canadian market, private label custom formulations take 12-16 weeks including R&D and stability testing, plus 60-90 days for NPN approval. Note: White label products can only be used in the US market due to Health Canada's brand-specific NPN requirements."
  },
  {
    question: "Can you help with both US and Canadian markets?",
    answer: "Yes! We specialize in both markets. Our services include FDA compliance for the US market and Health Canada requirements including NPN applications and bilingual labeling for Canada."
  },
  {
    question: "What certifications do your manufacturing partners have?",
    answer: "All our manufacturing partners are GMP-certified and FDA-registered. Many also hold additional certifications like NSF, USP, and Health Canada site licenses. We match you with partners that meet your specific certification requirements."
  },
  {
    question: "What product formats can you manufacture?",
    answer: "We can manufacture all major supplement formats including capsules (vegetable and gelatin), tablets (standard and chewable), softgels, gummies, powders (bulk and single-serve stick packs), liquids (tinctures and syrups), and effervescent tablets. Each format has different MOQs, with most starting at just 1000 units."
  },
  {
    question: "Do you provide packaging and labeling design services?",
    answer: "Yes! Our marketing and branding services include professional label design that meets all FDA and Health Canada requirements, product photography, packaging design, and complete brand identity development. We can handle everything from minimalist designs to premium packaging solutions that help your products stand out on shelves."
  },
  {
    question: "What payment terms and financing options are available?",
    answer: "We offer flexible payment terms including 50% deposit with balance on delivery, Net 30 for established businesses, and can connect you with financing partners for larger orders. We also offer milestone-based payments for custom formulation projects, making it easier to manage cash flow as you launch and scale your brand."
  }
];

const metaDescription = "Complete supplement manufacturing and go-to-market services. Low MOQs, custom formulation, NPN applications, fulfillment, and more. Launch your brand with expert support.";
const metaTitle = "Manufacturing & Go-to-Market Services | Nutricraft Labs";
---

<BaseLayout title={metaTitle} description={metaDescription}>
  <Header />

  <!-- Enhanced Hero Section -->
  <section class="relative bg-gradient-to-b from-mint-50 to-white py-16 md:py-24">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <ScrollAnimation>
        <div class="text-center max-w-4xl mx-auto">
          <div class="inline-flex items-center gap-2 bg-mint-100 text-mint-800 px-4 py-2 rounded-full text-sm font-semibold mb-6">
            <span>End-to-End Supplement Solutions</span>
          </div>
          <h1 class="text-4xl md:text-5xl lg:text-6xl font-bold text-gray-900 mb-6">
            Complete Supplement Solutions:<br/>
            <span class="text-mint-600">From Formula to Market</span>
          </h1>
          <p class="text-xl text-gray-600 mb-8">
            Manufacturing Excellence + Go-to-Market Expertise = Your Success
          </p>

          <!-- Value Props -->
          <div class="flex flex-wrap justify-center gap-4 mb-10">
            <div class="flex items-center gap-2 bg-mint-50 px-3 py-1 rounded-full">
              <svg class="w-5 h-5 text-mint-600" fill="currentColor" viewBox="0 0 20 20">
                <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
              </svg>
              <span class="text-gray-700 font-medium text-sm">1000 Unit MOQs</span>
            </div>
            <div class="flex items-center gap-2 bg-mint-50 px-3 py-1 rounded-full">
              <svg class="w-5 h-5 text-mint-600" fill="currentColor" viewBox="0 0 20 20">
                <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
              </svg>
              <span class="text-gray-700 font-medium text-sm">20+ Successful Projects</span>
            </div>
            <div class="flex items-center gap-2 bg-mint-50 px-3 py-1 rounded-full">
              <svg class="w-5 h-5 text-mint-600" fill="currentColor" viewBox="0 0 20 20">
                <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
              </svg>
              <span class="text-gray-700 font-medium text-sm">End-to-End Support</span>
            </div>
          </div>

          <!-- Dual CTAs -->
          <div class="flex flex-col sm:flex-row gap-4 justify-center">
            <a href="#manufacturing" class="inline-flex items-center justify-center px-8 py-4 bg-mint-600 text-white font-semibold rounded-lg hover:bg-mint-700 transition-all duration-300 transform hover:scale-105 shadow-lg">
              <span class="ml-2">Start Manufacturing</span>
            </a>
            <a href="#go-to-market" class="inline-flex items-center justify-center px-8 py-4 bg-white text-mint-600 font-semibold rounded-lg hover:bg-mint-50 transition-all duration-300 transform hover:scale-105 shadow-lg border-2 border-mint-600">
                <span class="ml-2">See Go-to-Market Services</span>
            </a>
          </div>
        </div>
      </ScrollAnimation>
    </div>
  </section>

  <!-- Service Categories Overview -->
  <section class="py-20 bg-gray-50">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="grid md:grid-cols-2 gap-8">
        <!-- Manufacturing Services Overview -->
        <ScrollAnimation>
          <div class="bg-gradient-to-br from-mint-50 to-white rounded-xl p-8 border border-mint-200">
            <div class="flex items-center gap-3 mb-4">
              <h2 class="text-2xl font-bold text-gray-900">Manufacturing Services</h2>
            </div>
            <p class="text-gray-600 mb-6">
              From formulation to production, we coordinate every aspect of supplement manufacturing through our partner network with the lowest MOQs in the industry.
            </p>
            <ul class="space-y-3">
              <li class="flex items-start gap-2">
                <svg class="w-5 h-5 text-mint-600 mt-0.5" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
                </svg>
                <span class="text-gray-700">Custom & stock formulations</span>
              </li>
              <li class="flex items-start gap-2">
                <svg class="w-5 h-5 text-mint-600 mt-0.5" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
                </svg>
                <span class="text-gray-700">GMP-certified production</span>
              </li>
              <li class="flex items-start gap-2">
                <svg class="w-5 h-5 text-mint-600 mt-0.5" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
                </svg>
                <span class="text-gray-700">Quality testing & compliance</span>
              </li>
            </ul>
          </div>
        </ScrollAnimation>

        <!-- Go-to-Market Services Overview -->
        <ScrollAnimation delay={100}>
          <div class="bg-gradient-to-br from-mint-50 to-white rounded-xl p-8 border border-mint-200">
            <div class="flex items-center gap-3 mb-4">
              <h2 class="text-2xl font-bold text-gray-900">Go-to-Market Services</h2>
            </div>
            <p class="text-gray-600 mb-6">
              Everything you need to successfully launch and scale your supplement brand in US and Canadian markets.
            </p>
            <ul class="space-y-3">
              <li class="flex items-start gap-2">
                <svg class="w-5 h-5 text-mint-600 mt-0.5" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
                </svg>
                <span class="text-gray-700">Regulatory compliance & NPN</span>
              </li>
              <li class="flex items-start gap-2">
                <svg class="w-5 h-5 text-mint-600 mt-0.5" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
                </svg>
                <span class="text-gray-700">Branding & marketing support</span>
              </li>
              <li class="flex items-start gap-2">
                <svg class="w-5 h-5 text-mint-600 mt-0.5" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
                </svg>
                <span class="text-gray-700">Fulfillment & distribution</span>
              </li>
            </ul>
          </div>
        </ScrollAnimation>
      </div>
    </div>
  </section>

  <!-- Manufacturing Services Section -->
  <section id="manufacturing" class="py-20 bg-white">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <ScrollAnimation>
        <div class="text-center mb-16">
          <div class="inline-flex items-center bg-mint-100 text-mint-800 px-4 py-2 rounded-full text-sm font-semibold mb-4">
            <span>Manufacturing Excellence</span>
          </div>
          <h2 class="text-3xl md:text-4xl font-bold text-gray-900 mb-4">
            Manufacturing Services
          </h2>
          <p class="text-lg text-gray-600 max-w-3xl mx-auto">
            Industry-leading manufacturing partnerships with the lowest MOQs and highest quality standards
          </p>
        </div>
      </ScrollAnimation>

      <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-8">
        {manufacturingServices.map((service, index) => (
          <ScrollAnimation delay={index * 100}>
            <div class={`bg-white rounded-xl p-8 shadow-lg hover:shadow-xl transition-all duration-300 h-full flex flex-col group ${service.isComingSoon ? 'opacity-90' : 'hover:scale-105'}`}>
              <div class="flex-grow">
                <div class="flex items-start justify-between mb-3">
                  <h3 class="text-2xl font-bold text-gray-900">{service.title}</h3>
                  {service.isComingSoon && (
                    <span class="inline-flex items-center px-2.5 py-0.5 rounded-full text-xs font-medium bg-gray-100 text-gray-800">
                      More Info Coming Soon
                    </span>
                  )}
                </div>

                {/* Timeline indicator */}
                <div class="inline-flex items-center gap-2 text-sm text-gray-600 mb-3">
                  <svg class="w-4 h-4" fill="currentColor" viewBox="0 0 20 20">
                    <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm1-12a1 1 0 10-2 0v4a1 1 0 00.293.707l2.828 2.829a1 1 0 101.415-1.415L11 9.586V6z" clip-rule="evenodd"></path>
                  </svg>
                  <span>{service.timeline}</span>
                </div>

                <p class="text-gray-600 mb-4">{service.description}</p>
                <ul class="space-y-2 mb-6">
                  {service.highlights.map(highlight => (
                    <li class="flex items-start gap-2">
                      <svg class="w-5 h-5 text-mint-600 mt-0.5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                        <path fill-rule="evenodd" d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z" clip-rule="evenodd"></path>
                      </svg>
                      <span class="text-gray-700">{highlight}</span>
                    </li>
                  ))}
                </ul>
              </div>
              <div class="mt-6">
                {service.isComingSoon ? (
                  <div class="inline-flex items-center text-gray-500 font-semibold">
                    <svg class="w-5 h-5 mr-2" fill="currentColor" viewBox="0 0 20 20">
                      <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm1-12a1 1 0 10-2 0v4a1 1 0 00.293.707l2.828 2.829a1 1 0 101.415-1.415L11 9.586V6z" clip-rule="evenodd"></path>
                    </svg>
                    More Info Coming Soon
                  </div>
                ) : (
                  <a href={service.href} class="inline-flex items-center text-mint-600 font-semibold hover:text-mint-700 transition-colors">
                    {service.cta}
                    <svg class="ml-2 w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"></path>
                    </svg>
                  </a>
                )}
              </div>
            </div>
          </ScrollAnimation>
        ))}
      </div>
    </div>
  </section>

  <!-- Go-to-Market Services Section -->
  <section id="go-to-market" class="py-20 bg-gray-50">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <ScrollAnimation>
        <div class="text-center mb-16">
          <div class="inline-flex items-center gap-2 bg-mint-100 text-mint-800 px-4 py-2 rounded-full text-sm font-semibold mb-4">
            <span>Launch & Scale Successfully</span>
          </div>
          <h2 class="text-3xl md:text-4xl font-bold text-gray-900 mb-4">
            Go-to-Market Services
          </h2>
          <p class="text-lg text-gray-600 max-w-3xl mx-auto">
            Everything you need to successfully launch, market, and distribute your supplements
          </p>
        </div>
      </ScrollAnimation>

      <div class="grid md:grid-cols-2 lg:grid-cols-2 gap-8">
        {goToMarketServices.map((service, index) => (
          <ScrollAnimation delay={index * 100}>
            <div class={`bg-white rounded-xl p-8 shadow-lg hover:shadow-xl transition-all duration-300 h-full flex flex-col group ${service.isComingSoon ? 'opacity-90' : 'hover:scale-105'}`}>
              <div class="flex-grow">
                <div class="flex items-start justify-between mb-3">
                  <h3 class="text-2xl font-bold text-gray-900">{service.title}</h3>
                  {service.isComingSoon && (
                    <span class="inline-flex items-center px-2.5 py-0.5 rounded-full text-xs font-medium bg-gray-100 text-gray-800">
                      More Info Coming Soon
                    </span>
                  )}
                </div>

                {/* Timeline indicator */}
                <div class="inline-flex items-center gap-2 text-sm text-gray-600 mb-3">
                  <svg class="w-4 h-4" fill="currentColor" viewBox="0 0 20 20">
                    <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm1-12a1 1 0 10-2 0v4a1 1 0 00.293.707l2.828 2.829a1 1 0 101.415-1.415L11 9.586V6z" clip-rule="evenodd"></path>
                  </svg>
                  <span>{service.timeline}</span>
                </div>

                <p class="text-gray-600 mb-4">{service.description}</p>
                <ul class="space-y-2 mb-6">
                  {service.highlights.map(highlight => (
                    <li class="flex items-start gap-2">
                      <svg class="w-5 h-5 text-mint-600 mt-0.5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                        <path fill-rule="evenodd" d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z" clip-rule="evenodd"></path>
                      </svg>
                      <span class="text-gray-700">{highlight}</span>
                    </li>
                  ))}
                </ul>
              </div>
              <div class="mt-6">
                {service.isComingSoon ? (
                  <div class="inline-flex items-center text-gray-500 font-semibold">
                    <svg class="w-5 h-5 mr-2" fill="currentColor" viewBox="0 0 20 20">
                      <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm1-12a1 1 0 10-2 0v4a1 1 0 00.293.707l2.828 2.829a1 1 0 101.415-1.415L11 9.586V6z" clip-rule="evenodd"></path>
                    </svg>
                    More Info Coming Soon
                  </div>
                ) : (
                  <a href={service.href} class="inline-flex items-center text-mint-600 font-semibold hover:text-mint-700 transition-colors">
                    {service.cta}
                    <svg class="ml-2 w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"></path>
                    </svg>
                  </a>
                )}
              </div>
            </div>
          </ScrollAnimation>
        ))}
      </div>
    </div>
  </section>

  <!-- Updated Process Section -->
  <section class="py-20 bg-white">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <ScrollAnimation>
        <div class="text-center mb-16">
          <h2 class="text-3xl md:text-4xl font-bold text-gray-900 mb-4">
            Your Complete Journey to Market
          </h2>
          <p class="text-lg text-gray-600 max-w-3xl mx-auto">
            Our proven 6-step process takes you from concept to successful market launch with expert guidance at every stage
          </p>
        </div>
      </ScrollAnimation>

      <div class="grid md:grid-cols-2 lg:grid-cols-6 gap-6 mt-12 items-stretch">
        {processSteps.map((step, index) => (
