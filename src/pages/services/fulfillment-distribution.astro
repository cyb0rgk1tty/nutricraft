const metaTitle = "Fulfillment & Distribution Services | 3PL Partners & Amazon FBA | Nutricraft";
const metaDescription = "Streamlined fulfillment and distribution solutions for supplement brands. 3PL partner network, D2C fulfillment, Amazon FBA setup, and inventory management. Setup in 2-3 weeks.";
---

<BaseLayout title={metaTitle} description={metaDescription}>
  <Fragment slot="head">
    <script type="application/ld+json">
      {JSON.stringify({
        "@context": "https://schema.org",
        "@type": "Service",
        "name": "Fulfillment & Distribution Services",
        "description": metaDescription,
        "provider": {
          "@type": "Organization",
          "name": "Nutricraft",
          "url": "https://nutricraft.com"
        },
        "areaServed": ["US", "CA"],
        "serviceType": "Logistics",
        "offers": {
          "@type": "Offer",
          "availability": "https://schema.org/InStock",
          "deliveryLeadTime": "P7D"
        }
      })}
    </script>
  </Fragment>

  <Header />

  <!-- Hero Section -->
  <section class="relative bg-gradient-to-b from-mint-50 to-white py-16 md:py-24">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="grid md:grid-cols-2 gap-12 items-center">
        <div>
          <div class="flex items-center gap-3 mb-4">
            <span class="bg-mint-600 text-white px-3 py-1 rounded-full text-sm font-semibold">Streamlined Logistics</span>
          </div>
          <h1 class="text-4xl md:text-5xl lg:text-6xl font-bold text-dark-900 mb-6">
            Fulfillment & Distribution
            <span class="block text-mint-600 text-3xl md:text-4xl lg:text-5xl mt-2">Scale Your Business</span>
          </h1>
          <p class="text-xl text-dark-600 mb-6">
            Professional fulfillment solutions through our vetted 3PL partner network.
            D2C fulfillment, Amazon FBA preparation, and inventory management systems.
          </p>
          <ul class="space-y-3 mb-8">
            <li class="flex items-center gap-3">
              <svg class="w-6 h-6 text-mint-600 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
              </svg>
              <span class="text-dark-700">Setup in 2-3 weeks with live operations</span>
            </li>
            <li class="flex items-center gap-3">
              <svg class="w-6 h-6 text-mint-600 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
              </svg>
              <span class="text-dark-700">3PL partner network across North America</span>
            </li>
            <li class="flex items-center gap-3">
              <svg class="w-6 h-6 text-mint-600 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
              </svg>
              <span class="text-dark-700">Amazon FBA setup (4-8 weeks for compliance)</span>
            </li>
          </ul>
          <div class="flex flex-col sm:flex-row gap-4">
            <a href="/contact" class="inline-flex items-center justify-center px-8 py-4 bg-mint-600 text-white font-semibold rounded-lg hover:bg-mint-700 transition-colors shadow-lg">
              Start Fulfillment Setup
              <svg class="ml-2 w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 8l4 4m0 0l-4 4m4-4H3"></path>
              </svg>
            </a>
            <a href="#process" class="inline-flex items-center justify-center px-8 py-4 bg-white text-mint-600 font-semibold rounded-lg border-2 border-mint-600 hover:bg-mint-50 transition-colors">
              View Process
              <svg class="ml-2 w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 14l-7 7m0 0l-7-7m7 7V3"></path>
              </svg>
            </a>
          </div>
        </div>
        <div class="relative">
          <div class="bg-white rounded-2xl shadow-xl p-8">
            <h3 class="text-2xl font-bold text-dark-900 mb-6">Fulfillment Solutions</h3>
            <div class="space-y-4">
              <div class="flex items-start gap-3">
                <span class="text-mint-600 flex-shrink-0">
                  <svg class="w-8 h-8" fill="currentColor" viewBox="0 0 20 20">
                    <path fill-rule="evenodd" d="M4 4a2 2 0 012-2h8a2 2 0 012 2v12a2 2 0 01-2 2H6a2 2 0 01-2-2V4zm3 1h6v4H7V5zm6 6H7v2h6v-2z" clip-rule="evenodd"/>
                  </svg>
                </span>
                <div>
                  <p class="font-semibold text-dark-900">3PL Partner Network</p>
                  <p class="text-sm text-dark-600">Vetted fulfillment centers nationwide</p>
                </div>
              </div>
              <div class="flex items-start gap-3">
                <span class="text-mint-600 flex-shrink-0">
                  <svg class="w-8 h-8" fill="currentColor" viewBox="0 0 20 20">
                    <path d="M3 1a1 1 0 000 2h1.22l.305 1.222a.997.997 0 00.01.042l1.358 5.43-.893.892C3.74 11.846 4.632 14 6.414 14H15a1 1 0 000-2H6.414l1-1H14a1 1 0 00.894-.553l3-6A1 1 0 0017 3H6.28l-.31-1.243A1 1 0 005 1H3zM16 16.5a1.5 1.5 0 11-3 0 1.5 1.5 0 013 0zM6.5 18a1.5 1.5 0 100-3 1.5 1.5 0 000 3z"/>
                  </svg>
                </span>
                <div>
                  <p class="font-semibold text-dark-900">D2C Fulfillment</p>
                  <p class="text-sm text-dark-600">Direct-to-consumer order processing</p>
                </div>
              </div>
              <div class="flex items-start gap-3">
                <span class="text-mint-600 flex-shrink-0">
                  <svg class="w-8 h-8" fill="currentColor" viewBox="0 0 20 20">
                    <path fill-rule="evenodd" d="M3 5a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1zM3 10a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1zM3 15a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1z" clip-rule="evenodd"/>
                  </svg>
                </span>
                <div>
                  <p class="font-semibold text-dark-900">Amazon FBA Ready</p>
                  <p class="text-sm text-dark-600">Complete FBA setup and optimization</p>
                </div>
              </div>
              <div class="flex items-start gap-3">
                <span class="text-mint-600 flex-shrink-0">
                  <svg class="w-8 h-8" fill="currentColor" viewBox="0 0 20 20">
                    <path d="M2 11a1 1 0 011-1h2a1 1 0 011 1v5a1 1 0 01-1 1H3a1 1 0 01-1-1v-5zM8 7a1 1 0 011-1h2a1 1 0 011 1v9a1 1 0 01-1 1H9a1 1 0 01-1-1V7zM14 4a1 1 0 011-1h2a1 1 0 011 1v12a1 1 0 01-1 1h-2a1 1 0 01-1-1V4z"/>
                  </svg>
                </span>
                <div>
                  <p class="font-semibold text-dark-900">Inventory Management</p>
                  <p class="text-sm text-dark-600">Real-time tracking and analytics</p>
                </div>
              </div>
            </div>
            <div class="mt-6 bg-mint-50 rounded-lg p-4">
              <p class="text-center text-mint-700 font-semibold">
                99.5% order accuracy with 24-hour processing
              </p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Fulfillment Services -->
  <section class="py-16 md:py-20 bg-white">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <ScrollAnimation>
        <div class="text-center mb-12">
          <h2 class="text-3xl md:text-4xl font-bold text-dark-900 mb-4">
            Complete Fulfillment Solutions
          </h2>
          <p class="text-lg text-dark-600 max-w-3xl mx-auto">
            From order processing to customer delivery, we provide end-to-end
            fulfillment solutions tailored to your business needs.
          </p>
        </div>
      </ScrollAnimation>

      <div class="grid md:grid-cols-2 gap-8">
        {fulfillmentServices.map((service, index) => (
          <ScrollAnimation delay={index * 100}>
            <div class="bg-gradient-to-br from-mint-50 to-white rounded-xl shadow-lg p-8 h-full flex flex-col">
              <div class="flex items-start gap-4 mb-6">
                <span class="text-4xl">{service.icon}</span>
                <div>
                  <h3 class="text-2xl font-bold text-dark-900 mb-2">{service.service}</h3>
                  <p class="text-dark-600">{service.description}</p>
                </div>
              </div>

              <div class="space-y-4 flex-grow">
                <div>
                  <h4 class="font-semibold text-dark-700 mb-2">Key Features:</h4>
                  <ul class="space-y-1">
                    {service.features.map(feature => (
                      <li class="flex items-center gap-2">
                        <svg class="w-4 h-4 text-mint-600 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                          <path fill-rule="evenodd" d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z" clip-rule="evenodd"></path>
                        </svg>
                        <span class="text-dark-600 text-sm">{feature}</span>
                      </li>
                    ))}
                  </ul>
                </div>

                <div>
                  <h4 class="font-semibold text-dark-700 mb-2">Benefits:</h4>
                  <ul class="space-y-1">
                    {service.benefits.map(benefit => (
                      <li class="flex items-center gap-2">
                        <svg class="w-4 h-4 text-mint-600 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                          <path fill-rule="evenodd" d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z" clip-rule="evenodd"></path>
                        </svg>
                        <span class="text-dark-600 text-sm">{benefit}</span>
                      </li>
                    ))}
                  </ul>
                </div>
              </div>
            </div>
          </ScrollAnimation>
        ))}
      </div>
    </div>
  </section>

  <!-- Distribution Channels -->
  <section class="py-16 md:py-20 bg-gray-50">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <ScrollAnimation>
        <div class="text-center mb-12">
          <h2 class="text-3xl md:text-4xl font-bold text-dark-900 mb-4">
            Distribution Channel Strategy
          </h2>
          <p class="text-lg text-dark-600 max-w-3xl mx-auto">
            Choose the right distribution mix to maximize your reach and profitability
            across different customer segments and markets.
          </p>
        </div>
      </ScrollAnimation>

      <div class="grid md:grid-cols-2 gap-8">
        {distributionChannels.map((channel, index) => (
          <ScrollAnimation delay={index * 100}>
            <div class="bg-white rounded-xl shadow-lg p-8">
              <h3 class="text-2xl font-bold text-dark-900 mb-3">{channel.channel}</h3>
              <p class="text-dark-600 mb-4">{channel.description}</p>

              <div class="space-y-4">
                <div>
                  <h4 class="font-semibold text-dark-700 mb-2">Key Advantages:</h4>
                  <ul class="space-y-1">
                    {channel.advantages.map(advantage => (
                      <li class="flex items-center gap-2">
                        <svg class="w-4 h-4 text-mint-600 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                          <path fill-rule="evenodd" d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z" clip-rule="evenodd"></path>
                        </svg>
                        <span class="text-dark-600 text-sm">{advantage}</span>
                      </li>
                    ))}
                  </ul>
                </div>

                <div>
                  <h4 class="font-semibold text-dark-700 mb-2">Fulfillment Options:</h4>
                  <div class="flex flex-wrap gap-2">
                    {channel.fulfillmentOptions.map(option => (
                      <span class="bg-mint-100 text-mint-700 px-2 py-1 rounded text-xs">
                        {option}
                      </span>
                    ))}
                  </div>
                </div>

                <div class="grid md:grid-cols-2 gap-3">
                  <div class="bg-gray-50 rounded-lg p-3">
                    <p class="text-sm font-semibold text-dark-700">Best For</p>
                    <p class="text-sm text-dark-600">{channel.bestFor}</p>
                  </div>
                  <div class="bg-gray-50 rounded-lg p-3">
                    <p class="text-sm font-semibold text-dark-700">Avg. Margin</p>
                    <p class="text-lg font-bold text-mint-600">{channel.averageMargin}</p>
                  </div>
                </div>
              </div>
            </div>
          </ScrollAnimation>
        ))}
      </div>
    </div>
  </section>

  <!-- Warehouse Network -->
  <section class="py-16 md:py-20 bg-white">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <ScrollAnimation>
        <div class="text-center mb-12">
          <h2 class="text-3xl md:text-4xl font-bold text-dark-900 mb-4">
            Strategic Warehouse Network
