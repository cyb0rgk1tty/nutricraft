const stockFormulas = [
  { category: "Weight Management", count: "25+", popular: ["Garcinia Cambogia", "Green Coffee Bean", "CLA Complex"] },
  { category: "Sports Nutrition", count: "30+", popular: ["Whey Protein", "BCAA", "Pre-Workout"] },
  { category: "Immune Support", count: "20+", popular: ["Vitamin C + Zinc", "Elderberry", "Mushroom Complex"] },
  { category: "Beauty & Wellness", count: "35+", popular: ["Collagen Peptides", "Biotin", "Hair Vitamins"] },
  { category: "Men's Health", count: "15+", popular: ["Testosterone Support", "Prostate Health", "Men's Multi"] },
  { category: "Women's Health", count: "20+", popular: ["Prenatal", "Menopause Support", "Women's Multi"] },
  { category: "General Health", count: "40+", popular: ["Multivitamin", "Omega-3", "Probiotics"] },
  { category: "Cognitive/Mood", count: "25+", popular: ["Nootropics", "Ashwagandha", "5-HTP"] }
