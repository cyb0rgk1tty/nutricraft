  { name: "Tablets", description: "Compressed & chewable", moq: "1000 units" },
  { name: "Softgels", description: "Oil-based formulas", moq: "1000 units" },
  { name: "Gummies", description: "Pectin & gelatin based", moq: "2500 units" },
  { name: "Powders", description: "Bulk & single-serve", moq: "500 units" },
