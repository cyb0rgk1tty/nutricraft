  { category: "Sports Nutrition", count: "30+", popular: ["Whey Protein", "BCAA", "Pre-Workout"], npnReady: true },
  { category: "Immune Support", count: "20+", popular: ["Vitamin C + Zinc", "Elderberry", "Mushroom Complex"], npnReady: true },
  { category: "Beauty & Wellness", count: "35+", popular: ["Collagen Peptides", "Biotin", "Hair Vitamins"], npnReady: true },
  { category: "Men's Health", count: "15+", popular: ["Testosterone Support", "Prostate Health", "Men's Multi"], npnReady: false },
  { category: "Women's Health", count: "20+", popular: ["Prenatal", "Menopause Support", "Women's Multi"], npnReady: true },
  { category: "General Health", count: "40+", popular: ["Multivitamin", "Omega-3", "Probiotics"], npnReady: true },
  { category: "Cognitive/Mood", count: "25+", popular: ["Nootropics", "Ashwagandha", "5-HTP"], npnReady: false }
];

// Process Timeline
const processSteps = [
  {
    step: 1,
    title: "Consultation",
    description: "Discuss your brand vision and requirements",
    timeline: "Day 1",
    deliverable: "Custom quote & recommendations"
  },
  {
    step: 2,
    title: "Formula Selection",
    description: "Choose from 200+ formulas or create custom",
    timeline: "Days 2-7",
    deliverable: "Final formula & pricing approval"
  },
  {
    step: 3,
    title: "Design & Compliance",
    description: "Label design and regulatory review",
    timeline: "Week 2-3",
    deliverable: "FDA-compliant labels & documentation"
  },
  {
    step: 4,
    title: "Production",
    description: "Manufacturing and quality testing",
    timeline: "Week 4-6",
    deliverable: "Finished products ready to ship"
  }
];

// Pricing Tiers
const pricingTiers = [
  {
    quantity: "1,000 units",
    whiteLabel: "$8-12/unit",
    privateLabel: "$12-18/unit",
    savings: "Minimum investment",
    popular: false
  },
  {
    quantity: "1,000 units",
    whiteLabel: "$7-10/unit",
    privateLabel: "$10-15/unit",
    savings: "15% cost savings",
    popular: true
  },
  {
    quantity: "5,000 units",
    whiteLabel: "$6-8/unit",
    privateLabel: "$8-12/unit",
    savings: "25% cost savings",
    popular: false
  },
  {
    quantity: "10,000+ units",
    whiteLabel: "$5-7/unit",
    privateLabel: "$7-10/unit",
    savings: "35%+ cost savings",
    popular: false
  }
];

// Canada vs US Market Comparison
const canadaVsUs = [
  {
    aspect: "Regulatory Approval",
    canada: "Pre-market NPN required",
    us: "Post-market notification",
    impact: "3-12 month approval process"
  },
  {
    aspect: "Label Requirements",
    canada: "Bilingual (English/French)",
    us: "English only",
    impact: "Specialized label design needed"
  },
  {
    aspect: "Dosage Limits",
    canada: "Strict maximum limits",
    us: "More flexible dosing",
    impact: "Formula adjustments may be needed"
  },
  {
    aspect: "Health Claims",
    canada: "Pre-approved claims only",
    us: "Structure/function claims allowed",
    impact: "Marketing copy restrictions"
  },
  {
    aspect: "Market Size",
    canada: "$5.7B CAD (2024)",
    us: "$50B USD (2024)",
    impact: "Less competition, growing market"
  },
  {
    aspect: "Growth Rate",
    canada: "6.9% CAGR",
    us: "5.6% CAGR",
    impact: "Faster growth in Canada"
  }
];

// Trust Badges & Certifications
const certifications = [
  { name: "FDA Registered", description: "Fully FDA-compliant facility" },
  { name: "GMP Certified", description: "Good Manufacturing Practices" },
  { name: "Health Canada", description: "Licensed for Canadian market" },
  { name: "ISO 9001:2015", description: "International quality standard" }
];

// Canadian Market Features
const canadianFeatures = [
  {
    title: "Bilingual Labeling",
    description: "Professional English/French labels that meet all Health Canada requirements"
  },
  {
    title: "NPN Applications",
    description: "Complete support for Natural Product Number applications and renewals"
  },
  {
    title: "Dosage Compliance",
    description: "All formulas meet Canadian maximum dosage limits and restrictions"
  },
  {
    title: "NHPD Monographs",
    description: "Pre-approved ingredients that comply with Health Canada monographs"
  }
];

// FAQ Items - Comprehensive from both pages
const faqItems = [
  {
    question: "What's the difference between private label and white label supplements?",
    answer: "Private label supplements are custom formulas created exclusively for your brand, giving you complete control over ingredients and formulation. White label supplements use our pre-made, market-tested formulas that you can quickly brand as your own. Private label takes 12-16 weeks and costs more, while white label can launch in 4-6 weeks at a lower cost. Both options start at just 1000 units minimum order."
  },
  {
    question: "What's your minimum order quantity (MOQ)?",
    answer: "Our minimum order quantity is just 1000 units for all products - 3-5x lower than industry standard. This low MOQ means you can start with an investment as low as $8,000 instead of the typical $30,000+ required elsewhere. This allows you to test the market and scale gradually without huge upfront costs."
  },
  {
    question: "How long does it take to launch my supplement brand?",
    answer: "White label products can be ready in 4-6 weeks since the formulas are already developed and tested. Private label custom formulations typically take 12-16 weeks from concept to delivery, including R&D, stability testing, and production. For the Canadian market, add 60-90 days if NPN approval is needed. We can expedite timelines for rush orders with our express manufacturing service."
  },
  {
    question: "Do I need an NPN to sell supplements in Canada?",
    answer: "Yes, all supplements sold in Canada require a Natural Product Number (NPN) from Health Canada. Many of our white label formulas already have NPNs or are NPN-ready, significantly reducing your time to market. We handle the entire application process for formulas that need new NPNs. The approval process typically takes 60-90 days for Class I products."
  },
  {
    question: "What are the bilingual labeling requirements for Canada?",
    answer: "Health Canada requires all supplement labels to be in both English and French. This includes product name, directions for use, warnings, and all mandatory label information. We provide professional bilingual label design and translation services to ensure full compliance. The 'Product Facts' table must be prominently displayed in both languages."
  },
  {
    question: "Can I sell the same products in both Canada and the US?",
    answer: "Yes, but you'll need different labels for each market. US labels don't require French text and have different regulatory requirements. Canadian products need NPN numbers and bilingual labels, while US products follow FDA guidelines. We can produce separate runs for each market or help you create dual-market compliant labels where possible."
  },
  {
    question: "Are your supplements FDA and Health Canada compliant?",
    answer: "Yes, all our supplements are manufactured in FDA-registered, GMP-certified facilities that also meet Health Canada requirements. We handle all regulatory compliance including proper labeling, structure-function claims review for the US market, and NPN applications for Canada. Our quality team ensures every batch meets or exceeds both FDA and Health Canada requirements."
  },
  {
    question: "What types of supplements can you manufacture?",
    answer: "We manufacture all major supplement formats including capsules (veggie and gelatin), tablets (standard and chewable), softgels, gummies, powders (bulk and stick packs), and liquids. We have 200+ stock formulas across categories like sports nutrition, weight management, immune support, beauty, and general wellness. Custom formulations are available for any supplement type."
  },
  {
    question: "How much does private label manufacturing cost?",
    answer: "White label supplements typically cost $8-12 per unit at our 1000 unit minimum, totaling $8,000-12,000 to start. Private label custom formulas cost $12-18 per unit at minimum quantity, plus $5,000-10,000 for initial R&D and testing. Prices decrease significantly with higher volumes - up to 35% savings at 10,000+ units. All quotes include manufacturing, basic packaging, and quality testing."
  },
  {
    question: "How long does the NPN application process take?",
    answer: "For Class I products (lower risk vitamins and minerals), NPN applications typically take 60-90 days. Class II and III products may take 3-12 months depending on complexity. Many of our white label formulas already have NPNs or use pre-cleared ingredients that qualify for faster approval through monograph compliance. We have a 92% first-time approval rate."
  },
  {
    question: "Do you offer fulfillment and dropshipping services?",
    answer: "Yes, we offer complete fulfillment services including inventory storage, pick-and-pack, and direct-to-consumer shipping for both US and Canadian markets. Our dropshipping program allows you to sell without holding inventory - we ship directly to your customers with your branding. This is perfect for e-commerce businesses wanting to test products or scale without warehouse costs."
  },
  {
    question: "Can I get samples before placing a full order?",
    answer: "Yes, we provide samples of our stock formulas so you can evaluate quality before committing. For white label products, sample sets are available for a small fee that's credited toward your final order. For private label custom formulations, we'll create small test batches during R&D for your approval. This ensures you're 100% satisfied with the product before full production."
  }
];

const metaTitle = "Private Label & White Label Supplement Manufacturing | Low MOQ | US & Canada | NutriCraft";
const metaDescription = "Launch your supplement brand with just 1000 units minimum. Private label custom formulas or white label ready-made products. FDA-registered, Health Canada compliant manufacturing. US and Canadian markets. Get your free quote in 24 hours.";
---

<BaseLayout title={metaTitle} description={metaDescription}>
  <Fragment slot="head">
    <link rel="canonical" href="https://nutricraft.com/services/private-white-label-manufacturing" />
    <script type="application/ld+json">
      {JSON.stringify({
        "@context": "https://schema.org",
        "@type": "Service",
        "name": "Private Label & White Label Supplement Manufacturing",
        "description": metaDescription,
        "provider": {
          "@type": "Organization",
          "name": "NutriCraft",
          "url": "https://nutricraft.com"
        },
        "areaServed": ["US", "CA"],
        "serviceType": ["Private Label Manufacturing", "White Label Manufacturing"],
        "offers": {
          "@type": "AggregateOffer",
          "lowPrice": "8000",
          "priceCurrency": "USD",
          "eligibleQuantity": {
            "@type": "QuantitativeValue",
            "minValue": 1000,
            "unitCode": "EA"
          }
        }
      })}
    </script>
  </Fragment>

  <Header />

  <!-- Hero Section -->
  <section class="relative bg-gradient-to-b from-mint-50 to-white overflow-hidden py-16 md:py-24">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="grid md:grid-cols-2 gap-12 items-center">
        <div>
          <h1 class="text-4xl md:text-5xl lg:text-6xl font-bold text-dark-900 mb-6">
            Private Label & White Label <span class="text-mint-600">Supplement Manufacturing</span>
          </h1>
          <p class="text-xl text-dark-600 mb-6">
            Launch your supplement brand in the US or Canada with just <strong>1000 units minimum order</strong>.
            Choose custom private label formulations or ready-made white label products.
            FDA & Health Canada compliant manufacturing.
          </p>
          <ul class="space-y-3 mb-8">
            <li class="flex items-center gap-3">
              <svg class="w-6 h-6 text-mint-600 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
              </svg>
              <span class="text-dark-700"><strong>1000 unit minimum</strong> (vs 5000+ elsewhere)</span>
            </li>
            <li class="flex items-center gap-3">
              <svg class="w-6 h-6 text-mint-600 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
              </svg>
              <span class="text-dark-700">200+ stock formulas or custom development</span>
            </li>
            <li class="flex items-center gap-3">
              <svg class="w-6 h-6 text-mint-600 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
              </svg>
              <span class="text-dark-700">US & Canadian market compliance</span>
            </li>
            <li class="flex items-center gap-3">
              <svg class="w-6 h-6 text-mint-600 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
              </svg>
              <span class="text-dark-700">White label ready in 4-6 weeks</span>
            </li>
          </ul>
          <div class="flex flex-col sm:flex-row gap-4">
            <a href="/contact" class="inline-flex items-center justify-center px-8 py-4 bg-mint-600 text-white font-semibold rounded-lg hover:bg-mint-700 transition-all duration-300 transform hover:scale-105 shadow-lg hover:shadow-xl">
              Get Free Quote in 24 Hours
              <svg class="ml-2 w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 8l4 4m0 0l-4 4m4-4H3"></path>
              </svg>
            </a>
            <a href="#comparison" class="inline-flex items-center justify-center px-8 py-4 bg-white text-mint-600 font-semibold rounded-lg border-2 border-mint-600 hover:bg-mint-50 transition-colors duration-300">
              View Product Catalog
              <svg class="ml-2 w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"></path>
              </svg>
            </a>
          </div>
        </div>
        <!-- Hero Images Container -->
        <div class="relative">
          <!-- Third Image (Bag) - Behind Left -->
          <div class="absolute top-0 -left-3/5 md:-left-1/2 animate-slide-in-left-delayed opacity-90 origin-center">
            <picture class="block">
              <source
                media="(max-width: 768px)"
                srcset="/images/bag-private-label-transparent-500.webp"
                type="image/webp"
              />
              <source
                media="(max-width: 768px)"
                srcset="/images/bag-private-label-transparent-500.png"
                type="image/png"
              />
              <source
                media="(min-width: 769px)"
                srcset="/images/bag-private-label-transparent-500.webp 500w, /images/bag-private-label-transparent-1000.webp 1000w, /images/bag-private-label-transparent-1500.webp 1500w"
                sizes="(max-width: 768px) 500px, (max-width: 1024px) 1000px, 1500px"
                type="image/webp"
              />
              <img
                src="/images/bag-private-label-transparent-500.png"
                srcset="/images/bag-private-label-transparent-500.png 500w, /images/bag-private-label-transparent-1000.png 1000w, /images/bag-private-label-transparent-1500.png 1500w"
                sizes="(max-width: 768px) 500px, (max-width: 1024px) 1000px, 1500px"
                alt="Private Label Supplement Bag Manufacturing"
                class="w-1/2 md:w-3/5 h-auto mx-auto"
                loading="eager"
                width="500"
                height="500"
                fetchpriority="high"
                decoding="sync"
              />
            </picture>
          </div>

          <!-- Second Image (Protein Bag) - Behind Right -->
          <div class="absolute top-0 -right-3/5 md:-right-1/2 animate-slide-in-right-delayed opacity-90 origin-center">
            <picture class="block">
              <source
                media="(max-width: 768px)"
                srcset="/images/protein-private-label-transparent-500.webp"
                type="image/webp"
              />
              <source
                media="(max-width: 768px)"
                srcset="/images/protein-private-label-transparent-500.png"
                type="image/png"
              />
              <source
                media="(min-width: 769px)"
                srcset="/images/protein-private-label-transparent-500.webp 500w, /images/protein-private-label-transparent-1000.webp 1000w, /images/protein-private-label-transparent-1500.webp 1500w"
                sizes="(max-width: 768px) 500px, (max-width: 1024px) 1000px, 1500px"
                type="image/webp"
              />
              <img
                src="/images/protein-private-label-transparent-500.png"
                srcset="/images/protein-private-label-transparent-500.png 500w, /images/protein-private-label-transparent-1000.png 1000w, /images/protein-private-label-transparent-1500.png 1500w"
                sizes="(max-width: 768px) 500px, (max-width: 1024px) 1000px, 1500px"
                alt="Private Label Protein Powder Manufacturing"
                class="w-1/2 md:w-3/5 h-auto mx-auto"
                loading="eager"
                width="500"
                height="500"
                fetchpriority="high"
                decoding="sync"
              />
            </picture>
          </div>

          <!-- First Image (Bottle) - Front -->
          <div class="relative z-10 animate-slide-in-right">
            <picture class="block">
              <source
                media="(max-width: 768px)"
                srcset="/images/private-label-transparent-500.webp"
                type="image/webp"
              />
              <source
                media="(max-width: 768px)"
                srcset="/images/private-label-transparent-500.png"
                type="image/png"
              />
              <source
                media="(min-width: 769px)"
                srcset="/images/private-label-transparent-500.webp 500w, /images/private-label-transparent-1000.webp 1000w, /images/private-label-transparent-1500.webp 1500w"
                sizes="(max-width: 768px) 500px, (max-width: 1024px) 1000px, 1500px"
                type="image/webp"
              />
              <img
                src="/images/private-label-transparent-500.png"
                srcset="/images/private-label-transparent-500.png 500w, /images/private-label-transparent-1000.png 1000w, /images/private-label-transparent-1500.png 1500w"
                sizes="(max-width: 768px) 500px, (max-width: 1024px) 1000px, 1500px"
                alt="Private Label and White Label Supplement Manufacturing"
                class="w-full h-auto"
                loading="eager"
                width="500"
                height="500"
                fetchpriority="high"
                decoding="sync"
              />
            </picture>
          </div>
          <!-- Trust Badges Below Image -->
          <div class="grid grid-cols-2 gap-4 mt-6">
            {certifications.map((cert) => (
              <div class="bg-white rounded-lg shadow-sm p-3 text-center">
                <h3 class="font-semibold text-dark-900 text-xs">{cert.name}</h3>
                <p class="text-xs text-dark-600">{cert.description}</p>
              </div>
            ))}
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Pain Points Solution Section -->
  <section class="py-16 md:py-20 bg-white">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <ScrollAnimation>
        <div class="text-center mb-12">
          <h2 class="text-3xl md:text-4xl font-bold text-dark-900 mb-4">
            We Solve Your Biggest Manufacturing Challenges
          </h2>
          <p class="text-lg text-dark-600 max-w-3xl mx-auto">
            Traditional supplement manufacturers make it nearly impossible for new brands to succeed.
            We've eliminated every barrier to entry for both US and Canadian markets.
          </p>
        </div>
      </ScrollAnimation>

      <div class="grid md:grid-cols-2 gap-8">
        {painPoints.map((point, index) => (
          <ScrollAnimation delay={index * 100}>
            <div class="bg-white rounded-xl p-6 shadow-md hover:shadow-lg transition-shadow">
              <div class="flex-1">
                <h3 class="text-xl font-bold text-dark-900 mb-3">{point.problem}</h3>
                <div class="grid grid-cols-2 gap-4">
                  <div class="bg-gray-50 rounded-lg p-3">
                    <p class="text-sm font-semibold text-dark-700 mb-1">Industry Standard</p>
                    <p class="text-sm text-dark-600">{point.traditional}</p>
                  </div>
                  <div class="bg-mint-50 rounded-lg p-3">
                    <p class="text-sm font-semibold text-mint-700 mb-1">Our Solution</p>
                    <p class="text-sm text-mint-600">{point.ourSolution}</p>
                  </div>
                </div>
                <div class="mt-3 text-center">
                  <span class="inline-flex items-center px-3 py-1 bg-mint-100 text-mint-700 rounded-full text-sm font-semibold">
                    {point.savings}
                  </span>
                </div>
              </div>
            </div>
          </ScrollAnimation>
        ))}
      </div>
    </div>
  </section>

  <!-- Private vs White Label Comparison -->
  <section id="comparison" class="py-16 md:py-20 bg-gradient-to-b from-mint-50 to-white">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <ScrollAnimation>
        <div class="text-center mb-12">
          <h2 class="text-3xl md:text-4xl font-bold text-dark-900 mb-4">
            Private Label vs White Label: Choose Your Path
          </h2>
          <p class="text-lg text-dark-600 max-w-3xl mx-auto">
            Both options start at just 1000 units. Choose based on your brand strategy and timeline.
          </p>
        </div>
      </ScrollAnimation>

      <ScrollAnimation>
        <div class="bg-white rounded-xl shadow-xl overflow-hidden">
          <div class="overflow-x-auto">
            <table class="w-full">
              <thead class="bg-mint-600 text-white">
                <tr>
                  <th class="px-6 py-4 text-left font-semibold">Comparison</th>
                  <th class="px-6 py-4 text-left font-semibold">Private Label (Custom)</th>
                  <th class="px-6 py-4 text-left font-semibold">White Label (Stock)</th>
                  <th class="px-6 py-4 text-left font-semibold">Best Choice</th>
                </tr>
