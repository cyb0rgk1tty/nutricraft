              <h3 class="text-xl font-bold text-dark-900 mb-2">{benefit.title}</h3>
              <p class="text-dark-600 text-sm">{benefit.description}</p>
            </div>
          </ScrollAnimation>
        ))}
      </div>
    </div>
  </section>

  <!-- Stock Formulas Section -->
