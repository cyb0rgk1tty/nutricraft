  </Fragment>

  <Header />

  <!-- Hero Section -->
  <section class="relative bg-gradient-to-br from-mint-50 via-white to-red-50 py-16 md:py-24">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
