      <div class="relative">
        <div class="absolute left-1/2 transform -translate-x-1/2 h-full w-1 bg-mint-200"></div>
        {timeline.map((step, index) => (
          <ScrollAnimation delay={index * 100}>
            <div class={`relative flex items-center mb-8 ${index % 2 === 0 ? 'justify-start' : 'justify-end'}`}>
              <div class={`w-5/12 ${index % 2 === 0 ? 'text-right pr-8' : 'text-left pl-8'}`}>
                <div class="bg-white rounded-lg p-6 shadow-md">
                  <div class="text-sm font-semibold text-mint-600 mb-1">{step.phase}</div>
                  <h3 class="text-lg font-bold text-dark-900 mb-1">{step.task}</h3>
                  <p class="text-sm text-dark-600">{step.status}</p>
                </div>
              </div>
              <div class="absolute left-1/2 transform -translate-x-1/2 w-4 h-4 bg-mint-600 rounded-full border-4 border-white shadow"></div>
            </div>
          </ScrollAnimation>
        ))}
      </div>
    </div>
