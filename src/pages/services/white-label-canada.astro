      </ScrollAnimation>

      <div class="grid md:grid-cols-2 lg:grid-cols-4 gap-6">
        {benefits.map((benefit, index) => (
          <ScrollAnimation delay={index * 100}>
            <div class="bg-gradient-to-br from-white to-red-50 rounded-xl p-6 shadow-md text-center">
