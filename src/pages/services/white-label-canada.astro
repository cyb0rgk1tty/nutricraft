          <p class="text-lg text-dark-600 max-w-3xl mx-auto">
            Navigate Canadian regulations with confidence. We handle all compliance requirements.
          </p>
        </div>
      </ScrollAnimation>

      <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-6">
        {complianceFeatures.map((feature, index) => (
          <ScrollAnimation delay={index * 100}>
            <div class="bg-white rounded-xl shadow-md p-6 hover:shadow-lg transition-shadow">
              <div class="text-3xl mb-3">{feature.icon}</div>
              <h3 class="text-xl font-bold text-dark-900 mb-2">{feature.title}</h3>
              <p class="text-dark-600">{feature.description}</p>
            </div>
          </ScrollAnimation>
        ))}
