const metaTitle = "Marketing & Brand Kit Services | Professional Branding for Supplements | NutriCraft";
const metaDescription = "Complete branding solutions for supplement companies. Product photography, label design, marketing collateral, and digital assets. Professional brand kits in 2-3 weeks.";
---

<BaseLayout title={metaTitle} description={metaDescription}>
  <Fragment slot="head">
    <link rel="canonical" href="https://nutricraft.com/services/marketing-brand-kit" />
    <script type="application/ld+json">
      {JSON.stringify({
        "@context": "https://schema.org",
        "@type": "Service",
        "name": "Marketing & Brand Kit Services",
        "description": metaDescription,
        "provider": {
          "@type": "Organization",
          "name": "NutriCraft",
          "url": "https://nutricraft.com"
        },
        "areaServed": ["US", "CA"],
        "serviceType": "Branding and Marketing",
        "offers": {
          "@type": "AggregateOffer",
          "lowPrice": "2500",
          "highPrice": "10000",
          "priceCurrency": "USD"
        }
      })}
    </script>
  </Fragment>

  <Header />

  <!-- Hero Section -->
  <section class="relative bg-gradient-to-br from-mint-50 via-white to-green-50 py-16 md:py-24">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="grid md:grid-cols-2 gap-12 items-center">
        <div>
          <div class="flex items-center gap-3 mb-4">
            <span class="bg-mint-600 text-white px-3 py-1 rounded-full text-sm font-semibold">Professional Branding</span>
