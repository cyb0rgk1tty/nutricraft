  </section>

  <!-- Branding Services -->
  <section class="py-16 md:py-20 bg-white">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <ScrollAnimation>
        <div class="text-center mb-12">
          <h2 class="text-3xl md:text-4xl font-bold text-dark-900 mb-4">
            Complete Branding Services
          </h2>
          <p class="text-lg text-dark-600 max-w-3xl mx-auto">
            From concept to completion, we create cohesive brand experiences
            that resonate with your target audience and drive sales.
          </p>
        </div>
      </ScrollAnimation>

      <div class="grid md:grid-cols-2 gap-8">
        {brandingServices.map((service, index) => (
          <ScrollAnimation delay={index * 100}>
            <div class="bg-gradient-to-br from-mint-50 to-white rounded-xl shadow-lg p-8">
              <div class="flex items-start gap-4 mb-6">
                <span class="text-4xl">{service.icon}</span>
                <div>
                  <h3 class="text-2xl font-bold text-dark-900 mb-2">{service.service}</h3>
                  <p class="text-dark-600">{service.description}</p>
                </div>
              </div>

              <div class="space-y-4">
                <div>
                  <h4 class="font-semibold text-dark-700 mb-2">What's Included:</h4>
                  <ul class="space-y-1">
                    {service.deliverables.map(deliverable => (
                      <li class="flex items-center gap-2">
                        <svg class="w-4 h-4 text-mint-600 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                          <path fill-rule="evenodd" d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z" clip-rule="evenodd"></path>
                        </svg>
                        <span class="text-dark-600 text-sm">{deliverable}</span>
                      </li>
                    ))}
                  </ul>
                </div>

                <div>
                  <h4 class="font-semibold text-dark-700 mb-2">File Formats:</h4>
                  <div class="flex flex-wrap gap-1">
                    {service.formats.map(format => (
                      <span class="bg-green-100 text-green-700 px-2 py-1 rounded text-xs">
                        {format}
                      </span>
                    ))}
                  </div>
                </div>

                <div class="bg-mint-100 rounded-lg px-3 py-2 inline-block">
                  <p class="text-sm font-semibold text-mint-800">Timeline: {service.timeline}</p>
                </div>
              </div>
            </div>
          </ScrollAnimation>
        ))}
      </div>
    </div>
  </section>

  <!-- Photography Styles -->
  <section class="py-16 md:py-20 bg-gradient-to-br from-green-50 to-white">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <ScrollAnimation>
        <div class="text-center mb-12">
          <h2 class="text-3xl md:text-4xl font-bold text-dark-900 mb-4">
            Professional Photography Styles
