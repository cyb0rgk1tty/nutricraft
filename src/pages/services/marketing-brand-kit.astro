          <p class="text-lg text-dark-600 max-w-3xl mx-auto">
            High-quality product photography tailored to your brand needs and marketing channels,
            from e-commerce listings to social media campaigns.
          </p>
        </div>
      </ScrollAnimation>

      <div class="grid md:grid-cols-2 gap-8">
        {photographyStyles.map((style, index) => (
          <ScrollAnimation delay={index * 100}>
            <div class="bg-white rounded-xl shadow-lg p-8">
              <h3 class="text-2xl font-bold text-dark-900 mb-3">{style.style}</h3>
              <p class="text-dark-600 mb-4">{style.description}</p>

              <div class="space-y-4">
